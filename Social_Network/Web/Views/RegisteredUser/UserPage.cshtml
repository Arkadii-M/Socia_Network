@{
    Layout = "~/Views/RegisteredUser/Register_Layout.cshtml";
}
<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <link type="text/css" rel="stylesheet" href="../../Content/Site.css" />
    <title></title>
</head>
<body>
    <div>
        <form method="post" action="">

            <table>
                <tr>
                    <td><p>Логін: </p></td>
                    <td>@ViewBag.User.User_Login </td>
                </tr>
                <tr>
                    <td><p>Ім'я: </p></td>
                    <td>@ViewBag.User.User_Name</td>
                </tr>
                <tr>
                    <td><p>Прізвище: </p></td>
                    <td>
                        @ViewBag.User.User_Last_Name
                    </td>
                </tr>
                <tr>
                    <td><p>Електронна пошта: </p></td>
                    <td>
                        @ViewBag.User.Email
                    </td>
                </tr>
                <tr>
                    <td><p>Інтереси: </p></td>
                    <td>
                        @foreach (var inter in ViewBag.User.Interests)
                        {
                            <p>@inter,</p>
                        }
                    </td>
                </tr>
                <tr>
                    <td><p>Друзі: </p></td>
                    <td>
                        @foreach (var friends in ViewBag.User.Friends_Ids)
                        {
                            <p>@friends , </p>
                        }
                    </td>
                </tr>
                <tr><td><input type="submit" value="Додати в друзі" /> </td><td></td></tr>
            </table>
            </form>
    </div>
</body>
</html>